{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/dona-ora",
      "dest": "/api/index.js"
    },
    {
      "src": "/diventa-volontario",
      "dest": "/api/index.js"
    },
    {
      "src": "/grazie",
      "dest": "/api/index.js"
    }
  ]
}